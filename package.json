{
  "name": "ps-monorepo",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apis/*",
    "apps/*",
    "modules/*"
  ],
  "scripts": {
    "save": "bash _bin/commit-all.sh . ",
    "build-turbo": "turbo run build",
    "build-lerna": "lerna run build",
    "dev-turbo": "turbo run dev --parallel"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  },
  "dependencies": {
    "next": "^12.3.4",
    "next-transpile-modules": "^9.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "use-clipboard-copy": "^0.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "lerna": "^6.1.0",
    "postcss": "^8.4.17",
    "turbo": "^1.6.3",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
