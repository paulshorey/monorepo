{
  "name": "ps-monorepo",
  "version": "0.0.179",
  "private": true,
  "workspaces": [
    "apis/*",
    "apps/*",
    "modules/*"
  ],
  "scripts": {
    "save": "bash _bin/commit-all.sh . ",
    "build": "turbo run build && turbo run test",
    "build-lerna": "lerna run build && lerna run test",
    "dev": "turbo run dev --parallel"
  },
  "dependencies": {
    "next": "^12.3.4",
    "next-transpile-modules": "^9.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "use-clipboard-copy": "^0.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "lerna": "^6.1.0",
    "postcss": "^8.4.17",
    "turbo": "^1.6.3",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
